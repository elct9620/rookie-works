CKEDITOR.plugins.addExternal('imgur', '/ckeditor/plugins/imgur/', 'plugin.js');

CKEDITOR.editorConfig = function (config) {
  config.extraPlugins = 'imgur';

  config.toolbar_mini = [
    ["Undo", "Redo", "-"],
    ["Bold",  "Italic", "-", "NumberedList", "BulletedList", "-", "Link", "Unlink", "Imgur"],
  ];
  config.toolbar = "mini";
  config.imgurClientID = '3e615942ad6e55e';
}
